 <h3 class="p">- ou -</h3>

 

    

<main>
    <% 
            let avisoErro = {"emailFisico":"", "senhaFisico":""}
            let msgErro = {"emailFisico":"", "senhaFisico":""}
            let listaErros = ""
                if(listaErros){
                    listaErros.errors.forEach(function(itemErro){
                        if(itemErro.path == "emailFisico"){
                            avisoErro.emailFisico = "erro";
                            msgErro.emailFisico = itemErro.msg
                        }
                        if(itemErro.path == "senhaFisico"){
                            avisoErro.senhaFisico = "erro";
                            msgErro.senhaFisico = itemErro.msg
                        }
                    })
                }
       %>
    <form action="/loginfisico" method="post" id="form" class="form">
        <div class="input-box">
            <input type="text" name="emailFisico" id="emailFisico" class="email" class="<%= avisoErro.emailFisico %>" required="required">
            <span class="span-input">Email</span><br>
            <span>
                <%= msgErro.emailFisico %>
            </span><br>
        </div>
        <div class="input-box">
            <input type="password" id="senhaFisico"  class="<%= avisoErro.senhaFisico %>" name="senhaFisico" required="required"/>
            <span class="span-input">Senha</span><br>
            <span>
                <%= msgErro.senhaFisico %>
            </span>
        </div>

        <a href="" class="esqueceu" id="tiu"> Esqueceu sua senha?</a>


        
    <div class="button">

        <button type="submit"  class="butao">Entrar</button>

        <button type="submit" class="butao">Entrar</button>

    </div>

   
    
       
